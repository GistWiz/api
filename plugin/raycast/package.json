{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gistwiz",
  "title": "GistWiz for GitHub Gists",
  "description": "GistWiz for GitHub Gists",
  "icon": "extension-icon.png",
  "author": "wilmoore",
  "owner": "wilmoore",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "qs",
      "title": "GistWiz QuickSearch",
      "description": "GistWiz QuickSearch",
      "mode": "view",
      "preferences": [
        {
          "title": "token",
          "name": "token",
          "type": "password",
          "label": "GitHub Personal Access Token",
          "required": true,
          "description": "GitHub Personal Access Token",
          "default": ""
        },
        {
          "title": "host",
          "name": "host",
          "type": "textfield",
          "label": "Host",
          "required": false,
          "description": "QuickSearch Server Hostname",
          "default": "api.gistwiz.io"
        },
        {
          "title": "autopaste",
          "name": "autopaste",
          "type": "checkbox",
          "label": "Auto Paste Clipboard Contents",
          "required": false,
          "description": "Auto Paste Clipboard Contents",
          "default": false
        },
        {
          "title": "defaultTerm",
          "name": "defaultTerm",
          "type": "textfield",
          "label": "Default Auto-Complete Term",
          "required": false,
          "description": "Default Auto-Complete Term",
          "default": ""
        }
      ]
    }
  ],
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "@raycast/api": "^1.86.1",
    "@raycast/utils": "^1.18.1",
    "node-fetch": "^3.3.2",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "latest",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "latest",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}